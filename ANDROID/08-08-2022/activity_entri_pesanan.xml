<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EntriPesanan"
    tools:layout_editor_absoluteX="-46dp"
    tools:layout_editor_absoluteY="19dp">

    <Spinner
        android:id="@+id/sp_id_menu"
        android:layout_width="214dp"
        android:layout_height="30dp"
        android:layout_marginTop="124dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Spinner
        android:id="@+id/sp_id_pelanggan"
        android:layout_width="214dp"
        android:layout_height="30dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sp_id_menu" />

    <Spinner
        android:id="@+id/sp_jumlah"
        android:layout_width="214dp"
        android:layout_height="30dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sp_id_pelanggan" />

    <Spinner
        android:id="@+id/sp_id_user"
        android:layout_width="214dp"
        android:layout_height="30dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sp_jumlah" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Id Menu"
        app:layout_constraintBottom_toTopOf="@+id/sp_id_menu"
        app:layout_constraintStart_toStartOf="@+id/sp_id_menu" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Id Pelanggan"
        app:layout_constraintBottom_toTopOf="@+id/sp_id_pelanggan"
        app:layout_constraintStart_toStartOf="@+id/sp_id_pelanggan" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Jumlah"
        app:layout_constraintBottom_toTopOf="@+id/sp_jumlah"
        app:layout_constraintStart_toStartOf="@+id/sp_jumlah" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Id User"
        app:layout_constraintBottom_toTopOf="@+id/sp_id_user"
        app:layout_constraintStart_toStartOf="@+id/sp_id_user" />

    <Button
        android:id="@+id/btn_simpan_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Simpan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sp_id_user" />

    <EditText
        android:id="@+id/id_pesanan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Id Pesanan"
        app:layout_constraintBottom_toTopOf="@+id/sp_id_menu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>